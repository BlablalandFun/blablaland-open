{
  "name": "@blablaland/core",
  "private": true,
  "version": "0.0.1",
  "author": {
    "name": "Yovach",
    "url": "https://blablaland.fun/",
    "email": "yovach.mail@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Yovach/blablaland-open.git"
  },
  "keywords": [],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Yovach/blablaland-open/issues"
  },
  "homepage": "https://github.com/Yovach/blablaland-open#readme",
  "devDependencies": {
    "turbo": "^1.0.24"
  },
  "workspaces": [
    "packages/**"
  ],
  "engines": {
    "node": ">=16.0.0"
  },
  "engineStrict": true,
  "turbo": {
    "baseBranch": "origin/main",
    "pipeline": {
      "build": {
        "dependsOn": [
          "^build"
        ],
        "outputs": [
          ".next/**",
          "dist/**"
        ]
      },
      "start": {
        "dependsOn": [
          "build"
        ],
        "outputs": []
      },
      "lint": {
        "outputs": []
      },
      "dev": {
        "cache": false
      }
    }
  },
  "scripts": {
    "dev": "turbo run dev",
    "start": "turbo run start"
  }
}
